<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_country_origin_tree" model="ir.ui.view">

        <field name="name">country.origin.list</field>
        <field name="model">country.origin</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="country"/> 
                <field name="short_code"/> 
                <field name="group"/> 
                
            </list>
        </field>
    </record>



    <record id="action_country_origin" model="ir.actions.act_window">
        <field name="name">Country Origin</field>
        <field name="res_model">country.origin</field>
        <field name="view_mode">list,form</field>
        <field name="target">current</field>
    </record>

</odoo>
